{
  "uid": "crypto",
  "title": "Crypto Prices",
  "time": { "from": "now-6h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "token",
        "type": "query",
        "datasource": "QuestDB",
        "query": "SELECT DISTINCT symbol FROM crypto_prices ORDER BY 1",
        "refresh": 2
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Price ($)",
      "datasource": "QuestDB",
      "targets": [
        {
          "rawSql": "SELECT time, price FROM crypto_prices WHERE symbol = '${token}' ORDER BY time",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "usd"
        }
      },
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 10 }
    }
  ],
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s"
}
